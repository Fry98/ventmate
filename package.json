{
  "name": "ventmate",
  "version": "1.0.0",
  "description": "It's not cheating if nobody figures it out",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "productName": "VentMate",
    "appId": "Fry.VentMate",
    "win": {
      "target": [
        "portable"
      ]
    },
    "portable": {
      "artifactName": "ventmate.exe"
    },
    "directories": {
      "output": "dist"
    }
  },
  "author": "Fry",
  "license": "MIT",
  "dependencies": {
    "@fry98/memoryjs": "^3.3.1"
  },
  "devDependencies": {
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1"
  }
}
